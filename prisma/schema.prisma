generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @default(autoincrement()) @id
  email     String   @unique
  name      String?
  password  String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
  avatar String?
}
